/////////////////////////////////////////// example  - 1 ////////////////////////////////////////////////////////////////////////

class first {
    public static void main(String[] args) {

        String str1 = "m";                                // the given string is from the provided string, so it return true.
 
        System.out.println(str1.matches("[manish]"));

    }
}

// output
// true


///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

///////////////////////////////////////////// example  - 2 //////////////////////////////////////////////////////////////////////////////////////////


class first {
    public static void main(String[] args) {

        String str1 = "Z";                                // the given string is from the provided string, so it return true.
 
        System.out.println(str1.matches("[manish]"));

    }
}

// output
// false

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

///////////////////////////////////////////// example  - 3 //////////////////////////////////////////////////////////////////////////////////////////



class first {
    public static void main(String[] args) {

        String str1 = "M";                   // here we have the capital M, but we want the smaller
 
        System.out.println(str1.matches("[manish]"));

    }
}

// output
// false


