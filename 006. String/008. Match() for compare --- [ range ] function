////////////////////////////////////// example  - 1 //////////////////////////////////////////////////////////////////////////////////////

class first {
    public static void main(String[] args) {

        String str1 = "A";       

        System.out.println(str1.matches("[a-z0-9A-Z]"));               //Here the given string is present int he match () range, otherwise return false

    }
}

// output
// true





///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////



////////////////////////////////////// example  - 2 /////////////////////////////////////////////////////////////////////////////////////////


class first {
    public static void main(String[] args) {

        String str1 = "a";       

        System.out.println(str1.matches("[a-z][0-9]"));        //Here the double [][] means - we have the string from both range, otherwise it will give an error.

    }
}

// output
// false


///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////



///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////



////////////////////////////////////// example  - 3 /////////////////////////////////////////////////////////////////////////////////////////


class first {
    public static void main(String[] args) {

        String str1 = "a5";       

        System.out.println(str1.matches("[a-z][0-9]")); //Here the given string is present int he match () range

    }
}

// output
// ture




